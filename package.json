    "@types/bcrypt": "^5.0.0",
    "@types/lodash": "^4.14.175",
    "@types/node": "^16.9.4",
    "@types/node-vault": "^0.9.13",
    "@types/qrcode": "^1.4.1",
    "@types/shelljs": "^0.8.9",
    "@types/uuid": "^8.3.1",
    "bcrypt": "^5.0.1",
    "fastify-sensible": "^3.2.0",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
