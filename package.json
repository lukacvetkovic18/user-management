{
  "name": "user-management",
  "version": "1.0.0",
  "description": "System for managing users' info and their belongings.",
  "main": "index.ts",
  "scripts": {
    "watch": "nodemon --watch './src/**/*.ts' --exec ts-node ./src/index.ts",
    "start": "pm2 start 'ts-node ./src/index.ts' --name index.ts",
    "stop": "pm2 delete index.ts",
    "redb": "npm run syncDb && npm run encryptDb && npm run migrateDb",
    "migrateDb": "ts-node ./src/scripts/dbMigrate.ts",
    "syncDb": "ts-node ./src/scripts/dbSync.ts",
    "encryptDb": "ts-node ./src/scripts/dbEncrypt.ts",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "build": "rm -rf dist && tsc --sourceMap -p ./",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Luka Cvetkovic",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.3.1",
    "@fastify/rate-limit": "^7.3.0",
    "@fastify/sensible": "^5.1.1",
    "@types/md5": "^2.3.1",
    "axios": "^0.22.0",
    "cardano-addresses": "^3.6.0",
    "cloudmersive-validate-api-client": "^1.4.7",
    "dotenv": "^10.0.0",
    "fastify": "^3.21.3",
    "fastify-acl-auth": "^1.4.0",
    "fastify-autoload": "^3.9.0",
    "fastify-bcrypt": "^1.0.0",
    "fastify-cors": "^6.0.3",
    "fastify-jwt": "^3.2.1",
    "fastify-oas": "^3.0.8",
    "fastify-plugin": "^3.0.0",
    "fastify-rate-limit": "^5.9.0",
    "fastify-sensible": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "mysql": "^2.18.1",
    "node": "^16.17.0",
    "node-vault": "^0.9.22",
    "nodemailer": "^6.7.8",
    "nvm": "^0.0.4",
    "pino-pretty": "^7.0.1",
    "qrcode": "^1.5.1",
    "reflect-metadata": "^0.1.13",
    "shelljs": "^0.8.4",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "typeorm": "^0.2.37",
    "typeorm-pagination": "^2.0.3",
    "uuid": "^8.3.2"
  },
